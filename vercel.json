{
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "redirects": [
    {
      "source": "/blog",
      "destination": "/blog/hvac",
      "permanent": true
    },
    {
      "source": "/air-conditioning/ac-repair-tucson",
      "destination": "/services/hvac/ac-repair",
      "permanent": true
    },
    {
      "source": "/air-conditioning/ac-installation-tucson",
      "destination": "/services/hvac/ac-installation",
      "permanent": true
    },
    {
      "source": "/plumber-tucson/water-heaters-tucson",
      "destination": "/services/plumbing/water-heater-installation",
      "permanent": true
    },
    {
      "source": "/plumber-tucson",
      "destination": "/services/plumbing",
      "permanent": true
    },
    {
      "source": "/drain-and-sewer/drain-cleaning-tucson",
      "destination": "/services/plumbing/drain-cleaning",
      "permanent": true
    },
    {
      "source": "/roofing-tucson-az",
      "destination": "/services/roofing",
      "permanent": true
    },
    {
      "source": "/solar-tucson",
      "destination": "/services/solar/solar-monitoring",
      "permanent": true
    },
    {
      "source": "/electrical-tucson/standby-generator-installation-and-repair",
      "destination": "/services/electrical/generator-installation",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        }
      ]
    },
    {
      "source": "/static/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
